---
const { class: className } = Astro.props;
---

<svg
  viewBox="-5 -5 95 95"
  class:list={["stroke-black", className]}
  preserveAspectRatio="none"
>
  <g stroke-width="0.5" stroke-linecap="round" stroke-linejoin="round">
    <line x1="0" y1="0" x2="0" y2="80"></line>
    <line x1="10" y1="0" x2="10" y2="80"></line>
    <line x1="20" y1="0" x2="20" y2="80"></line>
    <line x1="30" y1="0" x2="30" y2="80"></line>
    <line x1="40" y1="0" x2="40" y2="80"></line>
    <line x1="50" y1="0" x2="50" y2="80"></line>
    <line x1="60" y1="0" x2="60" y2="80"></line>
    <line x1="70" y1="0" x2="70" y2="80"></line>
    <line x1="80" y1="0" x2="80" y2="80"></line>

    <line x1="0" y1="0" x2="80" y2="0"></line>
    <line x1="0" y1="10" x2="80" y2="10"></line>
    <line x1="0" y1="20" x2="80" y2="20"></line>
    <line x1="0" y1="30" x2="80" y2="30"></line>
    <line x1="0" y1="40" x2="80" y2="40"></line>
    <line x1="0" y1="50" x2="80" y2="50"></line>
    <line x1="0" y1="60" x2="80" y2="60"></line>
    <line x1="0" y1="70" x2="80" y2="70"></line>
    <line x1="0" y1="80" x2="80" y2="80"></line>

    <circle cx="10" cy="0" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="60" cy="10" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="60" cy="20" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="60" cy="30" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="60" cy="40" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="50" cy="20" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="50" cy="50" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="50" cy="60" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="50" cy="70" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="30" cy="50" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="70" cy="50" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="80" cy="50" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="10" cy="70" r="4.5" fill="white" stroke="#aaa"></circle>
    <circle cx="20" cy="30" r="4.5" fill="white" stroke="#aaa"></circle>

    <circle cx="0" cy="0" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="50" cy="0" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="60" cy="0" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="80" cy="0" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="80" cy="60" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="0" cy="10" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="0" cy="60" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="20" cy="50" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="30" cy="70" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="40" cy="20" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="40" cy="40" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="40" cy="50" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="50" cy="10" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="50" cy="30" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="50" cy="40" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="60" cy="50" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="60" cy="60" r="4.5" fill="black" stroke="black"></circle>
    <circle cx="70" cy="60" r="4.5" fill="black" stroke="black"></circle>
  </g>
</svg>
